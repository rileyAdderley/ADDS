#include <iostream>
#include "Monkey.h"

Monkey::Monkey() {
    name = "Monkey";
    result = ["Ninja"] = 1;
    result = ["Robot"] = 1;
    
}

std::string getResult(std:: string opponent ) {
    return result[opponent];
}

std::string getName( ) {
    return name;
}